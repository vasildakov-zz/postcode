{
    "name": "vasildakov/postcode",
    "description": "UK Postcode",
    "keywords": ["uk", "post", "postcode", "php"],
    "homepage": "https://github.com/vasildakov/postcode",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7|^5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/php-code-coverage": "^3.0|^4.0"
    },
    "suggest": {
        "vasildakov/postcode-validator": "For validating UK postcode",
        "vasildakov/postcode-parser": "For parsing UK postcode",
        "vasildakov/postcode-doctrine": "For integration with Doctrine ORM"
    },
    "autoload": {
        "psr-4": {
            "VasilDakov\\Postcode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakov\\Postcode\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
